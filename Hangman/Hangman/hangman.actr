(goal isa goal state guess_letter)
(goal2 isa goal state reinforce-right-guess)
(goal3 isa goal state GameOverCheck)
(goal4 isa goal state reset)
)


;; time offset and number of references

;; At the start of the model, retrieve any decision



(p retrieve-letter
=goal>
isa goal
state guess_letter
==>
=goal>
state anounce_decision
+retrieval>
isa letter
used false
-action>
)

(p make-decision
=goal>
isa goal
state anounce_decision
=retrieval>
isa letter
value =decision
==>
=goal>
state modify-chunks
letteris =decision
=retrieval>
used true
+action>
isa move
choice =decision
)

(p modify-chunks
=goal>
state modify-chunks
=retrieval>
==>
-retrieval>
=goal>
isa goal
state recording-result)


(p get-feedback
=goal>
isa goal
state recording-result
letteris =decision
=action>
isa move
feedback =result
firstLetter =firstLetter
firstLetterKnown =firstLetterKnown
wordSize =wordSize
==>
=goal>
isa goal
state check-if-right
ifLastLetterGood =result
letteris =decision
letter1 =firstLetter
firstLetterKnown =firstLetterKnown
length =wordSize

)


(p reinforce-right-guess
=goal>
isa goal
state check-if-right
ifLastLetterGood right
letteris =decision
letter1 =firstLetter
firstLetterKnown =firstLetterKnown
length =wordSize
==>
=goal>
isa goal
state clear-imaginal
letter1 =firstLetter
firstLetterKnown =firstLetterKnown
length =wordSize
+imaginal>
isa letter
value =decision
used true

)

(p clear-imaginal
=goal>
isa goal
state clear-imaginal
letter1 =firstLetter
firstLetterKnown =firstLetterKnown
length =wordSize
==>
-imaginal>
=goal>
isa goal
state if-known
letter1 =firstLetter
firstLetterKnown =firstLetterKnown
length =wordSize
-action>

)


(p ignore-wrong-guess
=goal>
isa goal
state check-if-right
ifLastLetterGood wrong
letter1 =firstLetter
firstLetterKnown =firstLetterKnown
length =wordSize
==>
=goal>
isa goal
state if-known
letter1 =firstLetter
firstLetterKnown =firstLetterKnown
length =wordSize

)


(p retrieve-from-first-letter
=goal>
isa goal
state if-known
firstLetterKnown true
letter1 =firstLetter
length =wordSize


==>

+retrieval>
isa word
letter1 =firstLetter
=goal>
isa goal
state guess-word
letter1 =firstLetter
length =wordSize
)

(p retrieve-from-length
=goal>
isa goal
state if-known
firstLetterKnown true
letter1 =firstLetter
length =wordSize

==>

+retrieval>
isa word
length =wordSize
=goal>
isa goal
state guess-word
letter1 =firstLetter
length =wordSize
)



(p yes
=goal>
isa goal
state guess-word
letter1 =firstLetter
length =wordSize

=retrieval>
isa word
letter1 =firstLetter
length =wordSize
value =guessWord
==>
=goal>
isa goal
state guess_letter
+action>
isa guess
guessWord =guessWord

)





(p if-first-letter-unknown
=goal>
isa goal
state if-known
firstLetterKnown false
==>
=goal>
isa goal
state guess_letter
)



(goal-focus goal)



